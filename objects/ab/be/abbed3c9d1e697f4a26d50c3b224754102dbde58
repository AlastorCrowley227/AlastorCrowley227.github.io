ServerEvents.recipes(event => {
    event.custom({
      type: 'create:milling',
      ingredients: [
          Ingredient.of('minecraft:nether_wart').toJson(),

      ],
      results: [

          Item.of('kubejs:wart_dust').toJson(),
          Item.of('kubejs:wart_dust').withChance('0.75'),
    
      ],
      processingTime: 100
    })
  }) 