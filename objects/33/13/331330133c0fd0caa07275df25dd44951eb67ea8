ServerEvents.recipes(event => {
    event.custom({
      type: 'create:milling',
      ingredients: [
          Ingredient.of('create:raw_zinc').toJson()
      ],
      results: [

          Item.of('create:crushed_zinc_ore').withChance('0.70'),
          Item.of('create:experience_nugget').withChance('0.75'),
    
      ],
      processingTime: 100
    })
  }) 