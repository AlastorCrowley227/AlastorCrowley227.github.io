ServerEvents.recipes(event => {
    event.remove({output:'create:steam_engine'})
    event.shaped('create:steam_engine', [
        'DBD',
        'BCB',
        'AAA'
      ], {
        A: 'minecraft:copper_block',
        B: 'create:shadow_steel',
        C: 'create:brass_block',
        D: 'createdeco:cast_iron_ingot'
      })
})