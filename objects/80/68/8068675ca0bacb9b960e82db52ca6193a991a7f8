ServerEvents.recipes(event => {
    event.custom({
      type: 'create:filling',
      ingredients: [
          Ingredient.of('kubejs:chromatic_dust').toJson(),
          Fluid.of('kubejs:sugoma', 40500).toJson()

      ],
      results: [

          Item.of('create:chromatic_compound').toJson()
    
      ],
      processingTime: 100
    })
  }) 