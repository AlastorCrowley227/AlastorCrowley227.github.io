ServerEvents.recipes(event => {
    event.custom({
      type: 'create:milling',
      ingredients: [
          Ingredient.of('minecraft:coal').toJson(),

      ],
      results: [

          Item.of('2x kubejs:carbon_dust').toJson(),
          Item.of('kubejs:carbon_dust').withChance('0.5'),
    
      ],
      processingTime: 100
    })
  }) 