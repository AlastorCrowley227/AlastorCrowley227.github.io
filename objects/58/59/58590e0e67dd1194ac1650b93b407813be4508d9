ServerEvents.recipes(event => {
    event.shaped('create:zinc_ingot', [
        'AAA',
        'AAA',
        'AAA'
      ], {
        A: 'create:zinc_nugget'
      }),
      event.shapeless('9x create:zinc_nugget', ['create:zinc_ingot']),
      event.shapeless('9x create:zinc_ingot', ['create:zinc_block'])
})